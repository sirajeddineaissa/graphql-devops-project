{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts": ["src/layouts"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@global/*": ["src/global/*"],
      "@slices/*": ["src/global/slices/*"],
      "@hooks": ["src/hooks"],
      "@utils/*": ["src/utils/*"],
      "@public/*": ["public/*"],
      "@styled": ["src/styled"],
      "@graphql": ["src/graphql"],
      "@bootstrap": ["src/styled/bootstrap"]
    }
  }
}